<h1>Sign in</h1>
<hr>
<div class="col-lg-6" style="margin-left: 20">
  <%= form_tag sessions_path do %>
  <div class="form-group">
    <%= text_field_tag :email, params[:email], placeholder: "Email", class:'form-control' %>
  </div>
  <div class="form-group">
    <%= password_field_tag :password, nil, placeholder: "Enter password...", class:'form-control' %>
  </div>
  <%= submit_tag "Sign in", class:'btn btn-primary' %>
</div>
<% end %>
</div>
